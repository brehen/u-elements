{
  "name": "@u-elements/u-elements",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Native HTML tags, just truly accessible",
  "homepage": "https://github.com/u-elements/u-elements/",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/u-elements/u-elements.git"
  },
  "bugs": {
    "url": "https://github.com/u-elements/u-elements/issues"
  },
  "customElements": "dist/custom-elements.json",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./custom-elements.json": "./dist/custom-elements.json",
    "./dist/custom-elements.json": "./dist/custom-elements.json",
    "./vscode.json": "./dist/vscode.json",
    "./dist/vscode.json": "./dist/vscode.json"
  },
  "scripts": {
    "build": "vite build && tsup src/index.ts --format esm,cjs --dts-only",
    "docs:build": "npm run build && vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "install:all": "npm i && npm i --prefix tests/qwik && npm i --prefix tests/react && npm i --prefix tests/solid && npm i --prefix tests/svelte && npm i --prefix tests/vue",
    "lint": "eslint",
    "prepublishOnly": "npm run build",
    "start": "vite --config ${npm_config_path:-.}/vite.config.ts",
    "start:qwik": "npm run start --path=tests/qwik",
    "start:react": "npm run start --path=tests/react",
    "start:solid": "npm run start --path=tests/solid",
    "start:svelte": "npm run start --path=tests/svelte",
    "start:vue": "npm run start --path=tests/vue",
    "test": "web-test-runner",
    "test:update": "npm run test -- --update-snapshots"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.0",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@remcovaes/web-test-runner-vite-plugin": "^1.2.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/runtime-dom": "^3.4.14",
    "@web/test-runner": "^0.18.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "^0.11.0",
    "custom-element-vs-code-integration": "^1.2.2",
    "esbuild": "^0.19.11",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "solid-js": "^1.8.11",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-cem": "^0.6.0",
    "vite-plugin-solid": "^2.8.2",
    "vitepress": "^1.0.0-rc.40",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "root": true,
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/recommended": {
        "allowDeclarations": true
      }
    }
  },
  "eslintIgnore": [
    "dist",
    "frameworks",
    "node_modules"
  ]
}
